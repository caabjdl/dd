<div class="row">
  <div class="col-md-12">
    <div class="portlet box light-grey">
      <div class="portlet-title">
        <div class="caption"><i class="fa fa-cogs"></i>用户<%= @user.name unless @user.nil? %>分配日志</div>
        <div class="actions">
        </div>
      </div>
      <div class="portlet-body">
        <div class="dataTables_wrapper form-inline" role="grid">
          <div class="table-responsive">
            <table class="table table-hover table-bordered">
              <thead>
                <tr>
                  <th>CaseId</th>
                  <th>时间</th>
                </tr>
              </thead>
              <tbody>
              <% @items.each do |f| %>
                <tr>
                  <td><%= link_to f[0], edit_aa_case_path(f[0]), :target => f[0] %></td>
                  <td><%= f[1].to_datetime.strftime('%m-%d %H:%M:%S') %></td>
                </tr>
              <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>