
<div class="row">
  <div class="col-md-12">
    <div class="portlet box light-grey">
      <div class="portlet-title">
        <div class="caption"><i class="fa fa-cogs"></i>回传照片列表</div>
        <div class="actions">
         
        </div>
      </div>
      <div class="portlet-body">
        <div class="dataTables_wrapper form-inline" role="grid">
          <div class="row">
            <div class="col-md-12 col-sm-12">
              <%= form_tag pingan_photos_path, :method => 'get' do %>
              <div class="dataTables_filter pull-left">
                <label>
                  关键字:<%= text_field_tag :search, params[:search], :class => "form-control input-small" %>
                </label>
                <%= submit_tag '查询', :name => nil, :class => 'btn' %>
               
              </div>
              <% end %>
            </div>
          </div>
          <div class="table-responsive">
            <table class="table table-hover table-bordered">
              <thead>
                <tr>
                  <th>编号</th>
                  <th>是否回传</th>
                  <th></th>
                </tr>
              </thead>
              <tbody>
              <% @pingan_photos.each do |f| %>
                <tr>
                  <td><%= f.aa_case_no %></td>
                  <td><%= f.is_returned ? '是' : '否' %></td>
                  <td>
                  </td>
                </tr>
              <% end %> 
              </tbody>
            </table>
            <%= paginate @pingan_photos %> 
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


